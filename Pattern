#include <stdio.h>

int main()
{
  for(int row=1; row<11; row++){
      for (int counter=0; counter<row; counter++){
        printf("+");
      }
      printf("\n");
  }
  
  for(int row=10; row>0; row--){
      for (int counter=0; counter<row; counter++){
        printf("+");
      }
      printf("\n");
  }
  
  return 0;
}
